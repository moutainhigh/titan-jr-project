<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.titanjr.fop.dao.TitanSysConfigDao">
	
    <select id="querySysConfigList" parameterType="com.titanjr.fop.entity.TitanSysConfig" 
    	resultType="com.titanjr.fop.entity.TitanSysConfig">
        SELECT
        cfgkey,
        cfgvalue,
        objkey,
        isactive
        FROM t_tfs_sysconfig
        WHERE isactive = 1
        <if test="objKey!=null">
            and objkey = #{objKey}
        </if>
        <if test="cfgKey!=null">
            and cfgkey = #{cfgKey}
        </if>
        ORDER BY objkey
    </select>

</mapper>
