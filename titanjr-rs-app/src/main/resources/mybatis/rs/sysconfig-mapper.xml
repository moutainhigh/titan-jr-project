<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.fangcang.titanjr.rs.dao.SysconfigDao">
    <select id="query" parameterType="map" resultType="com.fangcang.titanjr.rs.entity.Sysconfig">
		SELECT 
			cfgkey,
			cfgvalue,
			objkey,
			isactive 
		FROM t_tfs_sysconfig
		WHERE isactive = 1
		<if test="objKey!=null">
		and objkey = #{objKey}
		</if>
		 ORDER BY objkey
    </select>

</mapper>
